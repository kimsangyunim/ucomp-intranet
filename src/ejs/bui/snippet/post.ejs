									<div class="comment-display">
										<div class="comment-write">
											<!-- submit-form -->
											<fieldset class="submit-form module-a">
												<!-- submit-list -->
												<div class="form-list">
													<legend>전송 서식</legend>
													<!-- submit-item -->
													<div class="form-item">
														<div class="form-body">
															<div class="form-area">
																<span class="form textarea module-a style-b type-line normal-04 medium flex" data-bui-form-value>
																	<textarea class="form-elem" placeholder="내용을 입력해주세요." oninput="buiFormResize(this);"></textarea>
																</span>
															</div>
														</div>
													</div>
													<!-- //submit-item -->
												</div>
												<!-- //form-list -->
											</fieldset>
											<!-- //submit-form -->
										</div>
										<div class="comment-list">
											<!-- comment-item -->
											<div class="comment-item">
												<div class="comment-wrap">
													<div class="comment-head">
														<!-- posts-account -->
														<div class="posts-account">
															<div class="account-figure"><span class="account-image"><img src="./../images/@temp/thumbnail_48x48.png" alt="" /></span></div>
															<div class="account-inform">
																<div class="account-head">
																	<strong class="account-name">홍길동</strong>
																	<strong class="account-titl">선임</strong>
																	<span class="account-divi">서비스운영본부</span>
																	<span class="account-dept">데이타 플랫폼 TFT팀</span>
																</div>
																<div class="account-util">
																	<div class="button-area">
																		<a href="#" class="btn module-a style-c type-line normal-04 x-small"><span class="btn-text">010-1234-5678</span></a>
																		<a href="#" class="btn module-a style-c type-line normal-04 x-small"><span class="btn-text">administrator@ucomp.co.kr</span></a>
																	</div>
																</div>
															</div>
