<!DOCTYPE html>
<html lang="ko">
<head>
<%-include('../_templates/_data.header.front.ejs', {
	pageStyle: 'bui.page.snippet.css',
	pageJavascript: ''
}) %>
</head>
<body>
<div id="wrap">
	<!-- page -->
	<div id="page">
<%-include('../_templates/_header.snippet.ejs') %>
		<!-- page-body -->
		<div class="page-body" id="local">
			<!-- local-head -->
			<div class="local-head">
				<h2 class="local-title" id="localTitle"><span class="local-name">snippet</span></h2>
			</div>
			<!-- //local-head -->
			<!-- local-body -->
			<div class="local-body" id="content">
				<!-- content-head -->
				<div class="content-head"><%- include('../_mixin/_title.content.ejs', {name: 'post'}) %></div>
				<!-- //content-head -->
				<!-- content-body -->
				<div class="content-body">
					<div class="section">
						<div class="section-body">
							<!-- subsection -->
							<div class="subsection">
								<div class="subsection-head"><%- include('../_mixin/_title.subsection.ejs', {name: 'module-a'}) %></div>
								<div class="subsection-body">
									<!-- post-display -->
									<div class="post-display module-a style-a type-a">
										<div class="post-list">
											<% for(var i = 0; i < 1; i++) { %>
												<%-include('../../front/_mixin/_post.item.notice.ejs') %>
											<% } %>
										</div>
									</div>
									<!--// post-display -->
								</div>
							</div>
							<!-- //subsection -->
							<!-- subsection -->
							<div class="subsection">
								<div class="subsection-head"><%- include('../_mixin/_title.subsection.ejs', {name: 'module-b'}) %></div>
								<div class="subsection-body">
									<!-- post-display -->
									<div class="post-display module-b style-a type-a">
										<div class="post-list">
											<% for(var i = 0; i < 1; i++) { %>
												<%-include('../../front/_mixin/_post.item.signoff.ejs') %>
											<% } %>
										</div>
									</div>
									<!-- //post-display -->
								</div>
							</div>
							<!-- //subsection -->
							<!-- subsection -->
							<div class="subsection">
								<div class="subsection-head"><%- include('../_mixin/_title.subsection.ejs', {name: 'module-c'}) %></div>
								<div class="subsection-body">
									<!-- data-display -->
									<div class="post-display module-c style-a type-c">
										<ul class="post-list">
											<li class="post-item">
												<div class="post-wrap">
													<div class="post-figure"><a href="#" class="post-thumbnail"><img src="./../../assets/images/@temp/img_receipt_01_enlarge.png" alt=""><span class="text">확대 보기</a></div>
													<!--
													<div class="post-inform">
														<div class="post-head"><p class="post-name"></p></div>
													</div> -->
												</div>
											</li>
											<li class="post-item">
												<div class="post-wrap">
													<div class="post-figure"><a href="#" class="post-thumbnail"><img src="./../../assets/images/@temp/img_receipt_01_enlarge.png" alt=""><span class="text">확대 보기</a></div>
												</div>
											</li>
										</ul>
									</div>
									<!-- //data-display -->
								</div>
							</div>
							<!-- //subsection -->
						</div>
					</div>
				</div>
				<!-- content-body -->
			</div>
			<!-- //local-body -->
		</div>
		<!-- //page-body -->
<%-include('../_templates/_footer.front.ejs') %>
	</div>
	<!-- //page -->
</div>
</body>
</html>